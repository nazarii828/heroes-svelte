{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "backend": "json-server --watch db.json --routes routes.json --port 5001 --middlewares ./node_modules/json-server-reset",
    "quick": "concurrently \"npm run backend\" \"npm run dev\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "concurrently": "^4.0.1",
    "json-server": "^0.15.1",
    "json-server-reset": "^1.0.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "sirv-cli": "^0.4.4",
    "svelte-preprocess": "^3.4.0",
    "svelte-routing": "^1.4.0"
  }
}
